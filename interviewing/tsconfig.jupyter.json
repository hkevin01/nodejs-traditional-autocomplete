{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "declaration": false,
        "outDir": "./.jupyter/compiled",
        "rootDir": "./notebooks"
    },
    "include": [
        "./notebooks/**/*",
        "../**/*.d.ts"
    ],
    "exclude": [
        "node_modules",
        ".jupyter/compiled"
    ]
}
